                    "margin": {
                        "top": "var(--wp--custom--spacer--l)"
                    },
                    "padding": {
                        "top": "var(--wp--custom--spacer--h)"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)",
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "fontStyle": "normal"
                }
            },
            "core/group": {
                "spacing": {
                    "margin": {
                        "top": "0 !important",
                        "right": "0 !important",
                        "bottom": "0 !important",
                        "left": "0 !important"
                    }
                }
            },
