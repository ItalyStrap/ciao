{
    "version": 1,
    "templateParts": [
        {
            "name": "header",
            "area": "header"
        },
        {
            "name": "singular-header",
            "area": "header"
        },
        {
            "name": "footer",
            "area": "footer"
        }
    ],
    "customTemplates": [
        {
            "name": "blank",
            "title": "Blank",
            "postTypes": [
                "page",
                "post"
            ]
        }
    ],
    "settings": {
        "color": {
            "custom": true,
            "link": true,
            "palette": [
                {
                    "slug": "text",
                    "color": "rgba(0,0,0,1.00)",
                    "name": "Black for text, headings, links"
                },
                {
                    "slug": "background",
                    "color": "rgba(255,255,255,1.00)",
                    "name": "White for body background"
                },
                {
                    "slug": "base",
                    "color": "rgba(57,134,224,1.00)",
                    "name": "Brand base color"
                }
            ],
            "gradients": [
                {
                    "slug": "black-to-white",
                    "gradient": "linear-gradient(160deg,var(--wp--preset--color--text),var(--wp--preset--color--background))",
                    "name": "Black to white"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#000000",
                        "#ffffff"
                    ],
                    "slug": "black-to-white",
                    "name": "Black to White"
                },
                {
                    "colors": [
                        "#3986e0",
                        "#ffffff"
                    ],
                    "slug": "base-to-white",
                    "name": "base-to-white"
                },
                {
                    "colors": [
                        "rgba(0,0,0,1.00)",
                        "#3986e0"
                    ],
                    "slug": "black-to-base",
                    "name": "black-to-base"
                },
                {
                    "colors": [
                        "#3986e0",
                        "rgba(0,0,0,1.00)"
                    ],
                    "slug": "base-to-black",
                    "name": "base-to-black"
                }
            ]
        },
        "typography": {
            "customFontSize": true,
            "customLineHeight": true,
            "fontSizes": [
                {
                    "slug": "base",
                    "size": "20px",
                    "name": "Base font size 16px"
                },
                {
                    "slug": "h1",
                    "size": "calc(var(--wp--preset--font-size--base) * 2.5)",
                    "name": "Used in H1 titles"
                },
                {
                    "slug": "h2",
                    "size": "calc(var(--wp--preset--font-size--base) * 2)",
                    "name": "Used in H2 titles"
                },
                {
                    "slug": "h3",
                    "size": "calc(var(--wp--preset--font-size--base) * 1.75)",
                    "name": "Used in H3 titles"
                },
                {
                    "slug": "h4",
                    "size": "calc(var(--wp--preset--font-size--base) * 1.5)",
                    "name": "Used in H4 titles"
                },
                {
                    "slug": "h5",
                    "size": "calc(var(--wp--preset--font-size--base) * 1.25)",
                    "name": "Used in H5 titles"
                },
                {
                    "slug": "h6",
                    "size": "var(--wp--preset--font-size--base)",
                    "name": "Used in H6 titles"
                }
            ],
            "fontFamilies": [
                {
                    "fontFamily": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
                    "slug": "base",
                    "name": "Default font family"
                },
                {
                    "fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace",
                    "slug": "monospace",
                    "name": "Font family for code"
                }
            ]
        },
        "spacing": {
            "customMargin": true,
            "customPadding": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "border": {
            "customColor": true,
            "customRadius": true,
            "customStyle": true,
            "customWidth": true
        },
        "custom": {
            "contentSize": "60vw",
            "wideSize": "80vw",
            "baseFontSize": "1rem",
            "spacer": {
                "base": "1rem",
                "v": "calc( var(--wp--custom--spacer--base) * 4 )",
                "h": "calc( var(--wp--custom--spacer--base) * 4 )",
                "s": "calc( var(--wp--custom--spacer--base) / 1.5 )",
                "m": "calc( var(--wp--custom--spacer--base) * 2 )",
                "l": "calc( var(--wp--custom--spacer--base) * 4 )"
            },
            "blockGap": {
                "base": "var(--wp--custom--spacer--base)",
                "s": "calc( var(--wp--custom--spacer--base) / 1.5 )",
                "m": "calc( var(--wp--custom--spacer--base) * 1.5 )",
                "l": "calc( var(--wp--custom--spacer--base) * 3 )",
                "xl": "calc( var(--wp--custom--spacer--base) * 4 )"
            },
            "lineHeight": {
                "s": "1.3",
                "m": "1.5",
                "l": "1.7"
            },
            "button": {
                "bg": "var(--wp--preset--color--base)",
                "text": "var(--wp--preset--color--background)"
            }
        },
        "blocks": {
            "core/group": []
        },
        "layout": {
            "contentSize": "var(--wp--custom--content-size)",
            "wideSize": "var(--wp--custom--wide-size)"
        }
    },
    "styles": {
        "border": [],
        "color": {
            "background": "var(--wp--preset--color--background)",
            "text": "var(--wp--preset--color--text)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--base)",
            "fontSize": "var(--wp--preset--font-size--base)",
            "fontStyle": "normal",
            "fontWeight": "400",
            "lineHeight": "var(--wp--custom--line-height--m)",
            "textDecoration": "none",
            "textTransform": "none"
        },
        "spacing": {
            "blockGap": "var(--wp--custom--block-gap--m)",
            "margin": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "padding": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--base)",
                    "background": "transparent"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-1)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-2)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-3)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-4)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-5)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-6)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--m)"
                    }
                }
            }
        },
        "blocks": {
            "core/paragraph": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--s)",
                        "bottom": "0px"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--text)"
                }
            },
            "core/button": {
                "border": {
                    "radius": "calc(var(--wp--preset--font-size--base)/4)",
                    "style": "solid",
                    "width": "1px"
                },
                "color": {
                    "background": "var(--wp--custom--button--bg)",
                    "text": "var(--wp--custom--button--text)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--base)",
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "textTransform": "uppercase"
                }
            },
            "core/code": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--l)"
                    },
                    "padding": {
                        "top": "var(--wp--custom--spacer--v)",
                        "right": "var(--wp--custom--spacer--h)",
                        "bottom": "var(--wp--custom--spacer--v)",
                        "left": "var(--wp--custom--spacer--h)"
                    }
                },
                "border": {
                    "color": "rgba(204,204,204,1.00)",
                    "radius": "0px",
                    "style": "solid",
                    "width": "1px"
                }
            },
            "core/quote": {
                "border": {
                    "color": "var(--wp--preset--color--text)",
                    "style": "solid",
                    "width": "0 0 0 1px"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--block-gap--l)"
                    },
                    "padding": {
                        "left": "var(--wp--custom--spacer--h)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "fontStyle": "normal"
                }
            },
            "core/post-date": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-6)"
                }
            },
            "core/separator": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                },
                "border": {
                    "color": "currentColor",
                    "style": "solid",
                    "width": "0 0 1px 0"
                }
            },
            "core/site-title": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-1)",
                    "fontWeight": "800"
                }
            },
            "core/site-tagline": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-3)",
                    "fontWeight": "600"
                }
            }
        }
    }
}
