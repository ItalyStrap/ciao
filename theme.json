        "spacing": {
            "blockGap": "2rem",
            "margin": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "padding": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            }
        },
        "elements": {
            "link": {
                "border": [],
                "color": {
                    "text": "var(--wp--preset--color--text)"
                },
                "spacing": [],
                "typography": []
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-1)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-2)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-3)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-4)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-5)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-6)"
                },
                "spacing": {
                    "blockGap": "4rem"
                }
            }
        },
        "blocks": {
            "overblocks/container": [],
            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "var(--wp--custom--spacer--v)",
                        "bottom": "var(--wp--custom--spacer--v)"
                    }
                }
            },
            "core/paragraph": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                }
            },
            "core/button": {
                "border": {
                    "radius": "calc(var(--wp--preset--font-size--base)/4)",
                    "style": "solid",
                    "width": "1px"
                },
                "color": {
                    "background": "var(--wp--preset--color--base)",
                    "text": "var(--wp--preset--color--background)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--base)",
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "text-transform": "uppercase"
                }
            },
            "core/code": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--monospace)"
                },
                "spacing": {
                    "padding": {
                        "left": "var(--wp--custom--spacer--h)",
                        "right": "var(--wp--custom--spacer--h)",
                        "top": "var(--wp--custom--spacer--v)",
                        "bottom": "var(--wp--custom--spacer--v)"
                    }
                },
                "border": {
                    "color": "rgba(204,204,204,1.00)",
                    "radius": "0px",
                    "style": "solid",
                    "width": "1px"
                }
            },
            "core/quote": {
                "border": {
                    "color": "var(--wp--preset--color--text)",
                    "style": "solid",
                    "width": "0 0 0 1px"
                },
                "spacing": {
                    "padding": {
                        "left": "var(--wp--custom--spacer--h)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--base)",
                    "fontStyle": "normal"
                }
            }
        }
